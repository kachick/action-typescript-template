{
  "name": "my-new-action",
  "version": "1.0.0",
  "description": "",
  "main": "lib/main.js",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc --project tsconfig.build.json",
    "format:fix": "dprint fmt",
    "format:check": "dprint check",
    "lint:fix": "eslint --fix {src,__tests__}/**/*.ts *.ts",
    "lint:check": "eslint {src,__tests__}/**/*.ts *.ts",
    "prepackage": "rimraf dist",
    "package": "ncc build",
    "test": "jest",
    "all": "npm run format:check && npm run lint:check && npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kachick/my-new-action.git"
